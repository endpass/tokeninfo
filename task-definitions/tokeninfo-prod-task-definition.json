{
    "executionRoleArn": "arn:aws:iam::573926759023:role/endpass-tokeninfo-prod-ecs-execution-role",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/prod/tokeninfo",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 8000
                }
            ],
            "cpu": 0,
            "environment": [
                {
                    "name": "ENV",
                    "value": "production"
                },
                {
                    "name": "TOKEN_LIST",
                    "value": "/data/tokens-eth.json"
                },
                {
                    "name": "TOKEN_IMAGE_DIR",
                    "value": "/data/tokens"
                }
            ],
            "mountPoints": [],
            "secrets": [],
            "volumesFrom": [],
            "image": "573926759023.dkr.ecr.us-east-1.amazonaws.com/prod/tokeninfo:latest",
            "essential": true,
            "readonlyRootFilesystem": true,
            "name": "tokeninfo"
        }
    ],
    "placementConstraints": [],
    "memory": "512",
    "taskRoleArn": "arn:aws:iam::573926759023:role/endpass-tokeninfo-prod-ecs-task-role",
    "family": "tokeninfo-prod-td",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256"
}
